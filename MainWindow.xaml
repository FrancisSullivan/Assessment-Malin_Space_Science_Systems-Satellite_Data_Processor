        <Button Content="Load Sensor Data" HorizontalAlignment="Left" Margin="17,62,0,0" VerticalAlignment="Top" Width="120" Grid.ColumnSpan="2"/>
        <ListView d:ItemsSource="{d:SampleData ItemCount=5}" Margin="17,87,438,24" Grid.ColumnSpan="2">
            <ListView.View>
                <GridView>
                    <GridViewColumn/>
                </GridView>
            </ListView.View>
        </ListView>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="79,164,212,24" Grid.Column="1"/>
        <Label Content="Sensor A" HorizontalAlignment="Left" Margin="76,0,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Grid.Column="1"/>
        <TextBox HorizontalAlignment="Left" Margin="184,114,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Grid.Column="1" IsEnabled="False"/>
        <Button Content="Search Iterative" HorizontalAlignment="Left" Margin="79,112,0,0" VerticalAlignment="Top" Width="100" Grid.Column="1"/>
        <TextBox HorizontalAlignment="Left" Margin="184,139,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Grid.Column="1" IsEnabled="False"/>
        <Button Content="Search Recursive" HorizontalAlignment="Left" Margin="79,137,0,0" VerticalAlignment="Top" Width="100" Grid.Column="1"/>
        <TextBox HorizontalAlignment="Left" Margin="184,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Grid.Column="1" IsEnabled="False"/>
        <Button Content="Sort Iterative" HorizontalAlignment="Left" Margin="79,62,0,0" VerticalAlignment="Top" Width="100" HorizontalContentAlignment="Center" Grid.Column="1"/>
        <TextBox HorizontalAlignment="Left" Margin="184,89,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Grid.Column="1" IsEnabled="False"/>
        <Button Content="Sort Recursive" HorizontalAlignment="Left" Margin="79,87,0,0" VerticalAlignment="Top" Width="100" Grid.Column="1"/>
        <TextBox HorizontalAlignment="Left" Margin="184,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Height="19" Grid.Column="1"/>
        <Label Grid.Column="1" Content="Search Target:" HorizontalAlignment="Left" Margin="79,31,0,0" VerticalAlignment="Top" Width="100"/>
        <ListBox d:ItemsSource="{d:SampleData ItemCount=5}" Margin="307,164,209,24" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Label Content="Sensor B" HorizontalAlignment="Left" Margin="304,0,0,0" VerticalAlignment="Top" FontSize="16" FontWeight="Bold" Grid.Column="1"/>
        <TextBox HorizontalAlignment="Left" Margin="412,114,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Grid.Column="1" IsEnabled="False" Grid.ColumnSpan="2"/>
        <Button Content="Search Iterative" HorizontalAlignment="Left" Margin="307,112,0,0" VerticalAlignment="Top" Width="100" Grid.Column="1"/>
        <TextBox HorizontalAlignment="Left" Margin="412,139,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Grid.Column="1" IsEnabled="False" Grid.ColumnSpan="2"/>
        <Button Content="Search Recursive" HorizontalAlignment="Left" Margin="307,137,0,0" VerticalAlignment="Top" Width="100" Grid.Column="1"/>
        <TextBox HorizontalAlignment="Left" Margin="412,64,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Grid.Column="1" IsEnabled="False" Grid.ColumnSpan="2"/>
        <Button Content="Sort Iterative" HorizontalAlignment="Left" Margin="307,62,0,0" VerticalAlignment="Top" Width="100" HorizontalContentAlignment="Center" Grid.Column="1"/>
        <TextBox HorizontalAlignment="Left" Margin="412,89,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Grid.Column="1" IsEnabled="False" Grid.ColumnSpan="2"/>
        <Button Content="Sort Recursive" HorizontalAlignment="Left" Margin="307,87,0,0" VerticalAlignment="Top" Width="100" Grid.Column="1"/>
        <TextBox HorizontalAlignment="Left" Margin="412,35,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="100" Height="19" Grid.Column="1" Grid.ColumnSpan="2"/>
        <Label Grid.Column="1" Content="Search Target:" HorizontalAlignment="Left" Margin="307,31,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
